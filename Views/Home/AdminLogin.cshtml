@model List<HelathProject_Domain.Status>
@{
    Layout = null;
}

<link href="https://cdn.jsdelivr.net/npm/bootstrap@5.0.0-beta1/dist/css/bootstrap.min.css" rel="stylesheet">

<body style="margin: 40px auto; width: 500px; height: 500px; background-image: url(https://thumbs.dreamstime.com/b/blackboard-text-health-wealth-stethoscope-blue-wooden-background-medecine-concept-blackboard-text-health-109546118.jpg)">
    <h2 style="background-color:white">AdminLogin</h2>
    <div class="row">
        <div class="col-sm-12">
            <table class="table" border="1" style="background-color:white">

                <tr>
                    <th style="font-size:20px;">Rating</th>
                    <th style="font-size:20px; color:black">Status</th>
                </tr>
                <tr>
                        @foreach (var item in Model)
                        {
                            Html.RenderPartial("ListOfStatusView", item);
                        }
                </tr>

            </table>

        </div>
    </div>
</body>

